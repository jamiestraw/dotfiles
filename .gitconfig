[alias]
  s = status -s
  l = log --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all

[core]
  excludesfile = /Users/jamie/.gitignore

[color]
  ui = true

[push]
  default = current
  followTags = true

